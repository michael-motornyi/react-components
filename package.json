    "@types/react-dom": "^18.0.9",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^9.1.0",
    "eslint": "^8.28.0",
    "eslint-config-next": "^13.0.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.4",
    "next": "^13.0.5",
